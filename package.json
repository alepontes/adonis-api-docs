{
  "name": "adonis-api-docs",
  "version": "0.2.2",
  "description": "Automatic API documentation creator for AdonisJS",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alepontes/adonis-api-docs.git"
  },
  "scripts": {
    "template-build": "cd template/ && next build && next export",
    "template-dev": "cd template/ && next dev",
    "template-start": "cd template/ && next start"
  },
  "keywords": [
    "adonisjs",
    "docs",
    "api"
  ],
  "files": [
    "/middleware",
    "/providers",
    "/*.md",
    "/*.json",
    "/paths.js",
    "/template/pages",
    "/template/public/favicon.ico",
    "/template/public/vercel.svg",
    "/template/styles",
    "/template/jsconfig.json",
    "/template/next.config.js"
  ],
  "author": "Ale Pontes <alexandrelopespontes@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/ignitor": "^2.0.8"
  },
  "dependencies": {
    "npm": "^7.6.1",
    "serve-static": "^1.14.1",
    "fs": "0.0.1-security",
    "next": "10.0.7",
    "path": "^0.12.7",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  }
}
